<?php
class Solution {

    /**
     * @param String $s
     * @return Integer
     */
    function lengthOfLastWord($s) {
        $new_arr = explode(" ",trim($s," "));
        return strlen($new_arr[count($new_arr)-1]);
    }
}
$data = new Solution();
$res = $data->lengthOfLastWord("   fly me   to   the moon  ");
?>
